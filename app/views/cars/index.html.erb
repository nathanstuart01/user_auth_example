<h1>My Cars</h1>
<table>
  <thead>
  <th>Cars</th>
  </thead>
    <tbody>
      <% @cars.each do |car| %>
        <tr>
          <td> <%= car.make %> </td>
          <td> <%= car.model %> </td>
          <td> <%= car.year %> </td>
          <td> <%= car.price %> </td>
          <%= link_to 'View Car', car_path(car) %> </td>
        </tr>
      <% end %>
    </tbody>
  </table>
  <%= link_to 'New Car', new_car_path %>
